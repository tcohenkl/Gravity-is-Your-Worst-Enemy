{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Rocket Game",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/rocket-game",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/lldb",
            "logging": { "trace": false, "traceResponse": false, "engineLogging": false },
            "osx": {
                "MIMode": "lldb"
            }
        }
    ]
}
